   "source": [
    "nullsQ1 = dataQ1.apply(lambda x:x.isnull().value_counts()).T[True]/len(dataQ1)*100\n",
    "nullsQ2 = dataQ2.apply(lambda x:x.isnull().value_counts()).T[True]/len(dataQ2)*100\n",
    "nullsQ3 = dataQ3.apply(lambda x:x.isnull().value_counts()).T[True]/len(dataQ3)*100\n",
    "nullsQ4 = dataQ4.apply(lambda x:x.isnull().value_counts()).T[True]/len(dataQ4)*100\n",
    "\n",
    "fig,(ax1,ax2,ax3,ax4) = plt.subplots(ncols=4,figsize=(12,8))\n",
    "\n",
    "nullsQ1.plot(title='Null values in Q1 data',kind='bar',xlabel='features',ylabel='% null values',fontsize=12,color='steelblue',ax=ax1)\n",
    "nullsQ2.plot(title='Null values in Q2 data',kind='bar',xlabel='features',ylabel='% null values',fontsize=12,color='steelblue',ax=ax2)\n",
    "nullsQ3.plot(title='Null values in Q3 data',kind='bar',xlabel='features',ylabel='% null values',fontsize=12,color='steelblue',ax=ax3)\n",
    "nullsQ4.plot(title='Null values in Q4 data',kind='bar',xlabel='features',ylabel='% null values',fontsize=12,color='steelblue',ax=ax4)\n",
    "\n",
    "plt.tight_layout()\n",
