   "source": [
    "### Create column 'day_night' ###\n",
    "night = ['00','01','02','03','04','05']\n",
    "morning = ['06','07','08','09','10','11']\n",
    "afternoon = ['12','13','14','15','16','17']\n",
    "evening = ['18','19','20','21','22','23']\n",
    "\n",
    "def day_night_handler(row):\n",
    "    if row['hour'] in night:\n",
    "        return 'night'\n",
    "    if row['hour'] in morning:\n",
    "        return 'morning'\n",
    "    if row['hour'] in afternoon:\n",
    "        return 'afternoon'\n",
    "    if row['hour'] in evening:\n",
    "        return 'evening'\n",
    "\n",
    "datax['day_night'] = datax.apply(day_night_handler,axis=1)\n",
    "\n",
    "# Check: show unique values\n",
    "print(datax['day_night'].unique())\n",
    "\n",
    "datax.head()"
   ]
  },
