   ],
   "source": [
    "# First define the first and third quartiles\n",
    "Q1 = data['birthyear'].quantile(0.25)\n",
    "Q3 = data['birthyear'].quantile(0.75)\n",
    "\n",
    "# Define the inter-quartile range\n",
    "IQR = Q3 - Q1\n",
    "# ... and the threshold value\n",
    "lowerL = Q1 - 1.5 * IQR\n",
    "\n",
    "print('The threshold for outliers lies at {}.'.format(int(lowerL)))\n",
    "\n",
    "# Impute 'left' outliers\n",
    "data.loc[data['birthyear'] < lowerL,'birthyear'] = lowerL"
   ]
  },
