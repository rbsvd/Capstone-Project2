   "source": [
    "### 1. Create column ride_length ###\n",
    "def sec_to_format(s):\n",
    "    h,s = divmod(int(s),3600)\n",
    "    m,s = divmod(s,60)\n",
    "    return f'{h:02}:{m:02}:{s:02}'\n",
    "\n",
    "diff = pd.to_datetime(data['end_time']) - pd.to_datetime(data['start_time'])\n",
    "data['ride_length'] = [sec_to_format(s) for s in diff.dt.total_seconds()]"
   ]
  },
