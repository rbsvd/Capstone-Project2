   "source": [
    "def row_handler(row):\n",
    "    if row['birthyear'] < 1970:\n",
    "        return '1960s'\n",
    "    if row['birthyear'] < 1980:\n",
    "        return '1970s'\n",
    "    if row['birthyear'] < 1990:\n",
    "        return '1980s'\n",
    "    if row['birthyear'] < 2000:\n",
    "        return '1990s'\n",
    "    if row['birthyear'] < 2010:\n",
    "        return '2000s'\n",
    "    if row['birthyear'] < 2020:\n",
    "        return '2010s'\n",
    "\n",
    "datax['ageBands'] = datax.apply(row_handler,axis=1)\n",
    "\n",
    "datax.head()"
   ]
  },
