     "text": [
      "The threshold for outliers lies at 2120.\n"
     ]
    }
   ],
   "source": [
    "# First define the first and third quartiles\n",
    "Q1 = datax['tripduration'].quantile(0.25)\n",
    "Q3 = datax['tripduration'].quantile(0.75)\n",
    "\n",
    "# Define the inter-quartile range\n",
    "IQR = Q3 - Q1\n",
    "# ... and the threshold value\n",
    "upperL = Q3 + 1.5 * IQR\n",
    "\n",
    "print('The threshold for outliers lies at {}.'.format(int(upperL)))\n",
    "\n",
    "# Impute 'left' outliers\n",
    "datax.loc[datax['tripduration'] > upperL,'tripduration'] = upperL"
   ]
  },
